
-- Prelude
--------------------------------------------------------------------------------

the (A : U) (x : A) = x;

id {A} (x : A) = x;

const {A B}(x : A)(_ : B) = x;

comp {A B C}(f : B → C)(g : A → B)(a : A) : C =
  f (g a);

modifyRef {A} (r : Ref A) (f : A → A) : Eff Σ() =
  do a ← read r;
  write r (f a);

Eq {A}(x y : A) = (P : A → U) → P x → P y;
refl {A}{x : A} : Eq x x = λ _ px. px;

Bool = (B : U) → B → B → B;
true  : Bool = λ _ t f. t;
false : Bool = λ _ t f. f;

--------------------------------------------------------------------------------

Monad (M : U → U) = Σ(
    ret  : {A} → A → M A
  , bind : {A B} → M A → (A → M B) → M B
);

MonadId : Monad (λ A. A) = (
    ret  = λ x. x
  , bind = λ a f. f a
);

--------------------------------------------------------------------------------

Gen (A : U) : U =
  {R} → (A → □ R) → □ R;

MonadGen : Monad Gen = (
    ret = λ a k. k a
  , bind = λ ga f k. ga λ a. (f a) k
);

runGen : {A} → Gen (□ A) → □ A
  = λ ga. ga (λ x. x);

gen : {A} → □ A → Gen (□ A) =
  λ a k. <let x = ~a; ~(k <x>)>;

--------------------------------------------------------------------------------

iter {A} (n : ℕ)(s : ℕ → A → A)(z : A) : A =
  ℕElim (λ {n}. s n) z n;

caseℕ {A}(n : ℕ) (s : ℕ → A) (z : A) : A =
  iter n (λ n _. s n) z;

add m n = iter m (λ _. suc) n;
mul m n = iter m (λ _. add n) 0;

test : ℕ → ℕ =
  λ x.  let m : ℕ = _;
        let p : Eq (caseℕ x (λ _. 0) 0) (caseℕ m (λ _. 0) 0) = refl;
	x;

uhh : Gen (□ ℕ) =
  open MonadGen;
  bind (gen <10>) λ x.
  bind (gen <20>) λ y.
  ret <add ~x ~y>;

kekaroo = ~(runGen uhh);



